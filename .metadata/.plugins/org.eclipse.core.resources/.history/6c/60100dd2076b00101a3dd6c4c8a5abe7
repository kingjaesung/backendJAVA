
<%@ page language="java" contentType="text/plain; charset=UTF-8"
	pageEncoding="UTF-8"%>


<%
int scoreInput = Integer.parseInt(request.getParameter("scoreInput"));
String gradeValue = request.getParameter("gradeInput");

if (scoreInput >= 90) {
	gradeValue = "A";
} else if (scoreInput >= 80) {
	gradeValue = "B";
} else if (scoreInput >= 70) {
	gradeValue = "C";
} else if (scoreInput >= 60) {
	gradeValue = "D";
} else {
	gradeValue = "F";
}
;

%>
